def debug(func):
    def inner(*args, **kwargs):
        print(f"\nЗапуск функции: {func.name}")
        print(f"Аргументы: {args if args else 'Нет'}")
        print(f"Ключевые аргументы: {kwargs if kwargs else 'Нет'}")
        return func(*args, **kwargs)
    return inner

@debug
def area(length, width):
    return length * width

# Пример вызова:
# result = area(7, 3)
# print(f"Площадь: {result}")
